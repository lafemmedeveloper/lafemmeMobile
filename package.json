{
  "name": "lafemmeApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "expert": "set ENVFILE=.env && npx react-native run-android --variant=expertDebug --appIdSuffix expertStaging",
    "client": "set ENVFILE=.env && npx react-native run-android --variant=clientDebug --appIdSuffix clientStaging",
    "android": "ENVFILE=.env.staging && react-native run-android --variant=clientDebug --appIdSuffix clientStaging",
    "iosC": "ENVFILE=.env react-native run-ios --scheme \"Client\"",
    "iosE": "ENVFILE=.env react-native run-ios --scheme \"Expert\" --simulator=\"iPhone 8\"",
    "iosCs": "ENVFILE=.env.staging react-native run-ios --scheme \"ClientStaging\"",
    "iosEs": "ENVFILE=.env react-native run-ios --scheme \"ExpertStaging\" --simulator=\"iPhone 8\"",
    "androidWC": "SET ENVFILE=.env react-native run-android --variant=clientDebug --appIdSuffix client",
    "androidC": "ENVFILE=.env react-native run-android --variant=clientDebug --appIdSuffix client",
    "androidE": "ENVFILE=.env react-native run-android --variant=expertDebug --appIdSuffix expert",
    "androidCs": "ENVFILE=.env.staging && react-native run-android --variant=clientStagingDebug --appIdSuffix clientstaging",
    "androidEs": "ENVFILE=.env.staging react-native run-android --variant=expertStagingDebug --appIdSuffix expertStaging",
    "start": "react-native start ",
    "test": "jest",
    "lint": "eslint .",
    "pod": "cd ios && pod install",
    "reset1": "git clean -f -x -d",
    "reset2": "git checkout -- .",
    "reset3": "watchman watch-del-all && rm -rf node_modules/ && yarn cache clean && yarn install",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:bundle": "cd android && ./gradlew bundleRelease",
    "xcode": "cd ios/ && open lafemmeApp.xcworkspace",
    "nvmInstall": "curl -o- https://raw.githubusercontent.com/creationix/nvm/v0.33.8/install.sh | bash",
    "nvmConfig": "export NVM_DIR=\"$HOME/.nvm\" &&  [ -s '$NVM_DIR/nvm.sh' ] && [ -s \"$NVM_DIR/nvm.sh\" ] && \\. \"$NVM_DIR/nvm.sh\" [ -s \"$NVM_DIR/bash_completion\" ] && . \"$NVM_DIR/bash_completion\"  ",
    "nvm": "nvm install 13.6.0",
    "fastlane:build": "bundle exec fastlane bump_version",
    "fastlane:patch": "bundle exec fastlane bump_version type:patch",
    "fastlane:minor": "bundle exec fastlane bump_version type:minor",
    "fastlane:major": "bundle exec fastlane bump_version type:major"
  },
  "fastlane:helper": {
    "build": "=> 3.40.1 (432) -> 3.40.1 (433)",
    "patch": "=> 3.40.1 (432) -> 3.40.2 (432)",
    "minor": "=> 3.40.1 (432) -> 3.41.0 (432)",
    "major": "=> 3.40.1 (432) -> 4.0.0 (432)"
  },
