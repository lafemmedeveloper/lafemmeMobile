{
  "name": "laFemmeMobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "iosC": "react-native run-ios --scheme \"lafemme\"",
    "iosE": "react-native run-ios --scheme \"lafemmeExpert\" --simulator=\"iPhone 8\"",
    "iosCs": "react-native run-ios --scheme \"lafemmeStaging\"",
    "iosEs": "react-native run-ios --scheme \"lafemmeExpertStaging\" --simulator=\"iPhone 8\"",
    "start": "react-native start --port 9988",
    "test": "jest",
    "lint": "eslint .",
    "pod": "cd ios && pod install",
    "reset1": "git clean -f -x -d",
    "reset2": "git checkout -- .",
    "reset3": "watchman watch-del-all && rm -rf node_modules/ && yarn cache clean && yarn install",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "xcode": "cd ios/ && open laFemmeMobile.xcworkspace"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.2",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-firebase/app": "6.2.0",
    "@react-native-firebase/auth": "6.2.0",
    "@react-native-firebase/firestore": "6.2.0",
    "@react-native-firebase/remote-config": "^6.2.0",
    "apisauce": "^1.1.1",
    "geolib": "^3.0.4",
    "material-table": "^1.54.1",
    "moment": "^2.24.0",
    "react": "16.9.0",
    "react-native": "0.61.3",
    "react-native-auto-height-image": "^1.1.3",
    "react-native-datepicker": "^1.7.2",
    "react-native-device-info": "^5.2.1",
    "react-native-fast-image": "^7.0.2",
    "react-native-gesture-handler": "^1.5.0",
    "react-native-google-places-autocomplete": "^1.3.9",
    "react-native-indicators": "^0.17.0",
    "react-native-iphone-x-helper": "^1.2.1",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-maps": "0.26.1",
    "react-native-modal": "^11.4.0",
    "react-native-reanimated": "^1.3.1",
    "react-native-star-rating": "^1.1.0",
    "react-native-swipe-list-view": "^2.0.3",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^4.0.10",
    "react-navigation-drawer": "^2.3.3",
    "react-navigation-stack": "^1.10.3",
    "react-navigation-tabs": "^2.5.6",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "rn-swipe-button": "^1.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/runtime": "^7.6.3",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.9.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.6.0",
    "jest": "^24.9.0",
    "jest-enzyme": "^7.1.2",
    "metro-react-native-babel-preset": "^0.56.3",
    "react-test-renderer": "16.9.0",
    "redux-devtools-extension": "^2.13.8"
  },
  "jest": {
    "preset": "react-native"
  }
}
